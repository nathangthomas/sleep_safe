<p id="notice"><%= notice %></p>

<h1 style="color:white">Events</h1>

<table class = 'table table-hover'>
  <thead>
    <tr>
      <td style="color:white">Event Name</td>
      <td style="color:white">Event Date </td>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td style="color:white"><%= link_to "#{event.name}", event_path(event) %></td>
        <td style="color:white"><%= event.date %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
